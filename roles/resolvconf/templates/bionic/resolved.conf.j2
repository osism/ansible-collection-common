[Resolve]
DNS={% for nameserver in resolvconf_nameserver %}{{ nameserver }}{% if not loop.last %} {% endif %}{% endfor %}
Domains={{ resolvconf_search }}
{%- if resolvconf_always_validate_dnssec is string %}
DNSSEC={{ resolvconf_always_validate_dnssec }}
{%- else %}
DNSSEC={{ 'yes' if resolvconf_always_validate_dnssec|bool else 'no' }}
{% endif %}
{%- if resolvconf_fallback_nameserver|length > 0 %}
FallbackDNS={% for nameserver in resolvconf_fallback_nameserver %}{{ nameserver }}{% if not loop.last %} {% endif %}{% endfor %}
{% endif %}
{%- if resolvconf_dns_over_tls is string %}
DNSOverTLS={{ resolvconf_dns_over_tls }}
{%- else %}
DNSOverTLS={{ 'yes' if resolvconf_dns_over_tls|bool else 'no' }}
{% endif %}
{%- if resolvconf_cache is string %}
Cache={{ resolvconf_cache }}
{%- else %}
Cache={{ 'yes' if resolvconf_cache|bool else 'no' }}
{% endif %}
